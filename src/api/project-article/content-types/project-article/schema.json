{
  "kind": "collectionType",
  "collectionName": "project_articles",
  "info": {
    "singularName": "project-article",
    "pluralName": "project-articles",
    "displayName": "Project Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "main": {
      "type": "blocks"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "project_articles"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "project_articles"
    },
    "projectinfo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.project-info"
    }
  }
}
